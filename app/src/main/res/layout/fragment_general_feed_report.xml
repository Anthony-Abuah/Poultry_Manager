            android:layout_height="40dp"
            android:layout_weight="1"
            android:layout_margin="2dp"
            android:layout_gravity="center"
            android:src="@drawable/ic_pdf"
            android:background="@color/green"
            android:elevation="25dp"
            />

        <ImageButton
